{
  "name": "@nextcloud/router",
  "version": "3.0.1",
  "description": "Utils for generating Nextcloud URLs",
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-router"
  },
  "license": "GPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "files": [
    "dist",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite --mode production build",
    "build:doc": "typedoc --out dist/doc lib && touch dist/doc/.nojekyll",
    "dev": "vite --mode development build",
    "watch": "vite --mode development build --watch",
    "lint": "eslint lib",
    "lint:fix": "eslint --fix lib",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "dependencies": {
    "@nextcloud/typings": "^1.9.1"
  },
  "devDependencies": {
    "@nextcloud/browserslist-config": "^3.0.1",
    "@nextcloud/eslint-config": "^8.4.2",
    "@nextcloud/vite-config": "^2.3.5",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^8.56.0",
    "happy-dom": "^17.5.6",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": "^10.0.0"
  }
}
